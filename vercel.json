{
    "version": 2,
    "builds": [
        {
            "src": "server/index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "client/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/client/$1"
        }
    ]
}